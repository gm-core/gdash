{
  "name": "gdash",
  "version": "5.0.0",
  "description": "This package.json is to install development utilities for gdash. You can ignore this if you are using gdash in your game.",
  "scripts": {
    "readme-toc": "markdown-toc -i README.md",
    "generate-docs": "node scripts/generate-docs.js && npm run readme-toc",
    "build": "npm run clean && node scripts/extract-scripts.js",
    "clean": "rimraf build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gm-core/gdash.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gm-core/gdash/issues"
  },
  "homepage": "https://github.com/gm-core/gdash#readme",
  "devDependencies": {
    "glob": "^7.1.2",
    "markdown-toc": "^1.2.0",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.6.2"
  }
}
